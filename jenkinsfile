pipeline (){
    agent any 
    stages(){
        stage('checkout stage'){
            steps(){
               git 'https://github.com/k-sreekanth/one.git'
            }
        }
        stage('test'){
            steps(){
               sh'mvn test'
            }
        }
         stage('build'){
            steps(){
               sh'mvn clean package'
            }
        }
         stages ("deploye") {
            steps(){
               sh'echo sreekanth'
            }
         }
    }
}
